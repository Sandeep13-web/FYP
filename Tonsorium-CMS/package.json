{
    "name": "node-express-sequelize-cms",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon src/app.js",
        "watch": "npm run development -- --watch",
        "dev": "npm run development",
        "development": "cross-env ENV_NODE=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "cross-env ENV_NODE=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env ENV_NODE=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
        "precommit-msg": "echo 'Checking files for possible flaws in coding standards.....'",
        "lint": "echo 'Checking files for possible flaws in coding standards.....' && eslint \"**/*.js\"",
        "lint-fix": "echo 'Fixing files for possible flaws in coding standards.....' && eslint \"**/*.js\" --fix",
        "docker": "docker-compose build && docker-compose up -d",
        "docker:prod": "docker-compose --file docker-compose.yml --file docker-compose.prod.yml --compatibility up --build",
        "test": "jest",
        "migrate": "npx sequelize db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
        "seed": "npx sequelize db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo",
        "make:middleware": "node src/core/commands/middleware",
        "make:route": "node src/core/commands/route",
        "make:validator": "node src/core/commands/validator",
        "make:controller": "node src/core/commands/controller",
        "make:service": "node src/core/commands/service",
        "make:model": "node src/core/commands/model",
        "generate:scafold": "node src/core/commands/scafold",
        "generate:migration": "npx sequelize-cli migration:create --name",
        "prepare": "husky install"
    },
    "_moduleAliases": {
        "@lib": "./src/helpers",
        "@config": "./src/config",
        "@constant": "./src/constants",
        "@models": "./src/models",
        "@errors": "./src/errors",
        "@transformers": "./src/transformers",
        "@routes": "./src/routes",
        "@responseFormat": "./src/responseFormat",
        "@baseController": "./src/controllers/backend/baseController",
        "@baseService": "./src/services/base.service.js",
        "@apiBaseController": "./src/controllers/api/apiBaseController.js",
        "@apiBaseService": "./src/services/api/api.base.service.js",
        "@serviceProvider": "./src/provider/service-provider",
        "@controller": "./src/controllers/backend",
        "@apiController": "./src/controllers/api",
        "@middleware": "./src/middlewares/backend",
        "@validators": "./src/validators",
        "@db": "./src/database/database",
        "@services": "./src/services",
        "@error": "./src/errors",
        "@events": "./src/events",
        "@emitter": "./src/listener",
        "@pagination": "./src/model/pagination",
        "@errorHandler": "./src/handler/error-handler",
        "@wrapNext": "./src/middlewares/backend/wrapNext.js",
        "@container": "./src/loaders/container.js",
        "@utils": "./src/utils"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.4",
        "awilix": "^7.0.3",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "~1.20.0",
        "ckeditor": "^4.11.4",
        "compression": "^1.7.4",
        "connect-flash": "latest",
        "connect-redis": "^6.0.0",
        "connect-session-sequelize": "^7.1.1",
        "convert-excel-to-json": "^1.7.0",
        "cookie-parser": "~1.3.5",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "dotenv": "^8.0.0",
        "ejs": "~2.5.5",
        "ejs-lint": "^1.2.1",
        "eslint": "^7.30.0",
        "events": "^3.2.0",
        "exceljs": "^1.13.0",
        "express": "4.18.1",
        "express-fileupload": "^1.1.4",
        "express-session": "latest",
        "express-validator": "^6.3.1",
        "helmet": "^4.4.1",
        "http-status-codes": "^2.2.0",
        "iconv-lite": "^0.6.3",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.11",
        "log4js": "~6.3.0",
        "mailgun-js": "^0.22.0",
        "method-override": "^3.0.0",
        "mime": "latest",
        "module-alias": "^2.2.2",
        "moment": "^2.24.0",
        "moment-precise-range": "^0.2.0",
        "moment-precise-range-plugin": "^1.3.0",
        "moment-timezone": "^0.5.28",
        "morgan": "~1.6.1",
        "mv": "^2.1.1",
        "node-constants": "latest",
        "nodemailer": "^6.6.3",
        "numeral": "latest",
        "oauth2orize": "^1.11.1",
        "passport": "latest",
        "passport-http": "^0.3.0",
        "passport-http-bearer": "^1.0.1",
        "passport-local": "^1.0.0",
        "passport-oauth2-client-password": "^0.1.2",
        "pg": "^8.6.0",
        "rand-token": "^0.4.0",
        "rate-limiter-flexible": "^2.2.4",
        "redis": "^3.1.2",
        "response-time": "^2.3.2",
        "sequelize": "^6.6.5",
        "sequelize-cli": "^6.2.0",
        "sequelize-paginate": "^1.1.6",
        "useragent": "^2.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "babel-eslint": "^10.1.0",
        "cross-env": "^6.0.3",
        "husky": "^8.0.0",
        "jest": "^28.1.0",
        "laravel-mix": "^5.0.1",
        "nodemon": "^2.0.7",
        "prettier": "2.2.1",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.24.0",
        "sass-loader": "^8.0.0",
        "supertest": "^6.2.3",
        "vue-template-compiler": "^2.6.14",
        "webpack-cli": "^4.7.2"
    }
}
