<div class="main-content side-content pt-5">
    <div class="container-fluid">
        <div class="inner-body">
            <!-- Page Header -->
            <div class="page-header sticky-header">
                <div>
                    <h2 class="main-content-title tx-24 mg-b-5"><%= translateLanguage(pageTitle, session.translationdata) %></h2>
                </div>
            </div>
            <!-- End Page Header -->
            <!-- Row -->
        </div>
        <!-- End Row -->
    </div>

    <div class="container-fluid">
        <div class="inner-body sticky-row">
            <div class="mt-2 mb-2">
                <% include ../partials/messages %>
            </div>
            <!-- Row -->
            <div class="row row-sm">
                <div class="col-lg-12">
                    <div class="card custom-card">
                        <div class="card-body">
                            <div class="table-responsive border">
                                <table class="table text__bold text-nowrap text-md-nowrap table-striped mg-b-0">
                                    <thead>
                                    <tr>
                                        <th><%= translateLanguage('User Name', session.translationdata) %></th>
                                        <th><%= translateLanguage('Services', session.translationdata) %></th>
                                        <th><%= translateLanguage('StaffId', session.translationdata) %></th>
                                        <th><%= translateLanguage('Day', session.translationdata) %></th>
                                        <th><%= translateLanguage('Start Date', session.translationdata) %></th>
                                        <th><%= translateLanguage('End Date', session.translationdata) %></th>
                                        <th><%= translateLanguage('Time', session.translationdata) %></th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <%
                                    if(docs.length > 0){
                                    docs.forEach(function(bookings) {
                                    %>

                                        <tr>
                                            <td><%= bookings.userName ? bookings.userName : nil %></td>
                                            <td><%= bookings.services ? bookings.services : nil %></td>
                                            <td><%= bookings.staffId ? bookings.staffId : nil %></td>
                                            
                                            <td ><%= bookings.day ? bookings.day : nil %></td>
                                            <td ><%= bookings.startDate ? formatDate(bookings.startDate, "YYYY-MM-DD") : nil %></td>
                                            <td ><%= bookings.endDate ? formatDate(bookings.endDate, "YYYY-MM-DD") : nil %></td>
                                            <td ><%= bookings.time ? bookings.time : nil %></td>
                                        </tr>

                                    <% });
                                    }else{
                                    %>
                                        <tr>
                                            <td colspan="7" style="text-align:center;">
                                                <%= translateLanguage('No Records found.', session.translationdata) %>
                                            </td>
                                        </tr>
                                    <% } %>

                                    </tbody>
                                </table>
                                <% if(docs.length > 0){ %>
                                    <% include ../partials/pagination %>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Row -->
        </div>
    </div>

</div>